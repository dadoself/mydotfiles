#!/bin/sh
curl "https://raw.githubusercontent.com/kdrag0n/base16-kitty/master/colors/base16-onedark.conf" -o ~/.config/kitty/theme.conf
curl "https://raw.githubusercontent.com/rkubosz/base16-sway/master/themes/base16-onedark.config" -o ~/.config/sway/theme.config
curl "https://raw.githubusercontent.com/mnussbaum/base16-waybar/master/colors/base16-onedark.css" -o ~/.config/waybar/theme.css
curl "https://raw.githubusercontent.com/chriskempson/base16-vim/master/colors/base16-onedark.vim" -o ~/.vim/colors/theme.vim
